DirectoryIndex index.php
Options +FollowSymlinks
Options -Indexes

RewriteEngine on

#RewriteRule . - [E=REWRITEBASE:/farmbiz_api/]
#RewriteBase /
 RewriteCond %{REQUEST_FILENAME} !-d
 RewriteRule ^(.*)/$ /$1 [L,R=301]

RewriteCond %{REQUEST_FILENAME} !-d
